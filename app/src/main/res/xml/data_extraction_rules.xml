<?xml version="1.0" encoding="utf-8"?>
<data-extraction-rules>
    <cloud-backup>
        <!-- Include all shared preferences -->
        <include domain="sharedpref" path="." />
        <!-- Exclude specific files -->
        <exclude domain="sharedpref" path="device.xml" />
    </cloud-backup>
    <device-transfer>
        <!-- Include all shared preferences -->
        <include domain="sharedpref" path="." />
        <!-- Exclude specific files -->
        <exclude domain="sharedpref" path="device.xml" />
    </device-transfer>
</data-extraction-rules>
